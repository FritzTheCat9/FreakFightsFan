@typeparam T

<MudSelect T=T
           Value=Value
           SelectedValuesChanged=OnSelectedValuesChanged
           For=For
           Label=@Label
           MultiSelection=false>
    @if (ItemsToSelect is not null)
    {
        @foreach (var item in ItemsToSelect)
        {
            <MudSelectItem T=T
                           Value=item />
        }
    }
    else
    {
        @ChildContent
    }
</MudSelect>
